</head>

<body>

 <!-- Navigation -->
    <nav class="navbar navbar-default navbar-fixed-top topnav" role="navigation">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!-- <a class="navbar-brand" href="#"> -->
                    <div class="col-lg-1">
                    <%= link_to root_path do %>
                    <%= image_tag("logotext.png", :alt => "LPLPainters") %> 
                    <% end %>
                </div> 

                   
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                   

                    <li>
                        <%=link_to "Home", root_path%>
                    </li>

                    <li>
                        <%=link_to "About", staticpages_about_path%> 
                    </li>
                    <li>
                        <%=link_to 'Inside', insideplaces_path%>
                    </li>
                    <li>
                        <%=link_to 'Outside', places_path %>
                    </li>
                    <li>
                        <%=link_to 'Contact', new_contact_path%>
                    <!-- </li>
                    <li class="social-facebook"><a href="#" target="_blank"><i class="fa fa-facebook"></i></a></li> -->
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>
    <div>
       <% if notice.present? %>
       <p class="notice"><%= notice %></p>
       <% end %>
       <% if alert.present? %>
       <p class="alert"><%= alert %></p>
       <% end %>
    </div>

      <%= yield %>

    <!-- Footer -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="list-inline text-center">
                        <li>
                             <%=link_to "Home", root_path%>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li>
                             <%=link_to "About", staticpages_about_path%>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li>
                            <%=link_to 'Inside', insideplaces_path%>  </a>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li>
                            <%=link_to 'Outside', places_path %>
                        </li>
                        <li class="footer-menu-divider">&sdot;</li>
                        <li>
                            <%=link_to 'Contact', new_contact_path%>
                        </li>
                    </ul>
                    <p class="copyright text-muted small text-center">Copyright &copy; LPL Painters 2017. All Rights Reserved</p>
                    <% if user_signed_in? %>
                       <p class="copyright text-muted small text-center"><%=link_to "Sign Out", destroy_user_session_path, method: :delete %></p>
                    <% else %>   
                    <p class="copyright text-muted small text-center"><%=link_to "Sign In", new_user_session_path%></p>
                    <% end %>
                </div>
            </div>
        </div>
    </footer>
   

</body>

</html>
